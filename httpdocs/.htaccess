# BEGIN Caching on image files
<filesMatch "\.(ico|gif|jpg|JPG|jpeg|png)$">
  ExpiresActive On
  ExpiresDefault "access 1 years"
  Header append Cache-Control "public"
</filesMatch>
# END Caching on image and font files
 
# BEGIN Caching on static files
<filesMatch "\.(js|json|geojson|ttf|ttc|woff|otf|svg)$">
  ExpiresActive On
  ExpiresDefault "access 1 years"
  Header append Cache-Control "public"
</filesMatch>
# END Caching on image and font files

# BEGIN Caching on web files
<filesMatch "\.(css|html|txt)$">
  ExpiresActive On
  ExpiresDefault "access 1 weeks"
  Header append Cache-Control "public"
</filesMatch>
# END Caching on web files

# BEGIN Removes whitespace-in-php-files error (fix for many xml, rss, plugin errors)
#php_value auto_prepend_file "/httpdocs/wejnswpwhitespacefix.php"
# END whitespace fix

# -----------------------------------------------------------------------
# Defining MIME types to ensure the web server actually knows about them.
# -----------------------------------------------------------------------
<IfModule mod_mime.c>
	AddType application/geojson             geojson
    AddType application/javascript          js
    AddType application/vnd.ms-fontobject   eot
    AddType application/x-font-ttf          ttf ttc
    AddType font/opentype                   otf
    AddType application/x-font-woff         woff
    AddType image/svg+xml                   svg svgz 
    AddEncoding gzip                        svgz
</Ifmodule>

# -----------------------------------------------------------------------
# Compressing output.
# -----------------------------------------------------------------------
<IfModule mod_deflate.c>
    AddOutputFilterByType DEFLATE text/html text/plain text/css application/json application/geojson
    AddOutputFilterByType DEFLATE application/javascript
    AddOutputFilterByType DEFLATE text/xml application/xml text/x-component
    AddOutputFilterByType DEFLATE application/xhtml+xml application/rss+xml application/atom+xml
    AddOutputFilterByType DEFLATE image/x-icon image/svg+xml application/vnd.ms-fontobject application/x-font-ttf font/opentype
</Ifmodule>



# Wordfence WAF
<IfModule mod_suphp.c>
	suPHP_ConfigPath '/var/www/vhosts/tourofcrete.com/httpdocs'
</IfModule>
<Files ".user.ini">
<IfModule mod_authz_core.c>
	Require all denied
</IfModule>
<IfModule !mod_authz_core.c>
	Order deny,allow
	Deny from all
</IfModule>
</Files>

# END Wordfence WAF

